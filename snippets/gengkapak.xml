<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Remotes -->
  <remote name="github" fetch="https://github.com" />
  <remote name="github-ssh" fetch="ssh://git@github.com" />
  <remote name="gitlab" fetch="https://gitlab.com" />
  <remote name="gitlab-ssh" fetch="ssh://git@gitlab.com" />
  <remote name="gengkapak" fetch="https://github.com/GengKapak-Project" revision="refs/heads/12.0" />
  <remote name="gengkapak-ssh" fetch="ssh://git@github.com/GengKapak-Project" revision="refs/heads/12.0" />

  <!-- Repositories -->
  <project path="manifest" name="platform_manifest" remote="gengkapak-ssh" />
  <project path="build/make" name="platform_build" remote="gengkapak-ssh" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/blueprint" name="platform_build_blueprint" remote="gengkapak-ssh" />
  <project path="build/soong" name="platform_build_soong" remote="gengkapak-ssh" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device repositories -->
  <project path="device/gengkapak/sepolicy" name="platform_device_gengkapak_sepolicy" remote="gengkapak-ssh" />
  <project path="device/qcom/sepolicy-legacy-um" name="platform_device_qcom_sepolicy-legacy-um" remote="gengkapak-ssh" />
  <project path="device/qcom/sepolicy_vndr" name="platform_device_qcom_sepolicy_vndr" remote="gengkapak-ssh" />

  <!-- External repositories -->
  <project path="external/colorkt" name="platform_external_colorkt" remote="gengkapak-ssh" />
  <project path="external/json-c" name="platform_external_json-c" remote="gengkapak-ssh" />
  <project path="external/noto-fonts" name="platform_external_noto-fonts" remote="gengkapak-ssh" />
  <project path="external/themelib" name="platform_external_themelib" remote="gengkapak-ssh" />
  <project path="external/tinycompress" name="platform_external_tinycompress" remote="gengkapak-ssh" />
  <project path="external/tinyxml" name="platform_external_tinyxml" remote="gengkapak-ssh" />

  <!-- Frameworks repositories -->
  <project path="frameworks/av" name="platform_frameworks_av" remote="gengkapak-ssh" />
  <project path="frameworks/base" name="platform_frameworks_base" remote="gengkapak-ssh" />
  <project path="frameworks/native" name="platform_frameworks_native" remote="gengkapak-ssh" />

  <!-- Packages repositories -->
  <project path="packages/apps/Launcher3" name="platform_packages_apps_Launcher3" remote="gengkapak-ssh" />
  <project path="packages/apps/Settings" name="platform_packages_apps_Settings" remote="gengkapak-ssh" />
  <project path="packages/apps/SettingsIntelligence" name="platform_packages_apps_SettingsIntelligence" remote="gengkapak-ssh" />
  <project path="packages/apps/SlavHouse" name="platform_packages_apps_SlavHouse" remote="gengkapak-ssh" />
  <project path="packages/apps/ThemePicker" name="platform_packages_apps_ThemePicker" remote="gengkapak-ssh" />

  <!-- Prebuilts repositories -->
  <project path="prebuilts/tools-extras" name="platform_prebuilts_tools-extras" remote="gengkapak-ssh" />

  <!-- System repositories -->
  <project path="system/apex" name="platform_system_apex" remote="gengkapak-ssh" />
  <project path="system/core" name="platform_system_core" remote="gengkapak-ssh" />
  <project path="system/sepolicy" name="platform_system_sepolicy" remote="gengkapak-ssh" />
  <project path="system/vold" name="platform_system_vold" remote="gengkapak-ssh" />

  <!-- Vendor repositories -->
  <project path="vendor/custom-preference" name="platform_vendor_custom-preference" remote="gengkapak-ssh" />
  <project path="vendor/gengkapak" name="platform_vendor_gengkapak" remote="gengkapak-ssh" />

</manifest>
