<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Remotes -->
  <remote name="github" fetch="https://github.com" />
  <remote name="github-ssh" fetch="ssh://git@github.com" />
  <remote name="gitlab" fetch="https://gitlab.com" />
  <remote name="gitlab-ssh" fetch="ssh://git@gitlab.com" />
  <remote name="gengkapak" fetch="https://github.com/GengKapak-Project" revision="refs/heads/12.0" />
  <remote name="gengkapak-ssh" fetch="ssh://git@github.com/GengKapak-Project" revision="refs/heads/12.0" />

  <!-- Repositories -->
  <project path="manifest" name="platform_manifest" remote="gengkapak-ssh" />
  <project path="build/make" name="platform_build" remote="gengkapak-ssh" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/blueprint" name="platform_build_blueprint" remote="gengkapak-ssh" />
  <project path="build/soong" name="platform_build_soong" remote="gengkapak-ssh" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

</manifest>
